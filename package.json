    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.4.19",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.21",
    "prisma": "^4.13.0",
    "supports-color": "^8.1.1",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
