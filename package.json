{
  "name": "housing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:dev": "NODE_ENV=dev prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix",
    "lint:strict": "eslint src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch --testPathPattern=src/",
    "prettier:fix": "prettier --write ./src",
    "all:fix": "npm run lint:fix && npm run prettier:fix",
    "test": "jest --silent --testPathPattern=src/",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "prepare": "node scripts/cesium.js"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.9.0",
    "@formspree/react": "3.0.0",
    "@prisma/client": "^4.13.0",
    "@react-three/fiber": "9.1.4",
    "@turf/mask": "7.2.0",
    "@vercel/speed-insights": "^1.2.0",
    "cesium": "1.131.0",
    "cloudinary": "^1.41.0",
    "clsx": "^2.1.1",
    "fs-extra": "^11.3.1",
    "geojson": "0.5.0",
    "lucide-react": "0.525.0",
    "mapbox-gl": "3.13.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-map-gl": "8.0.4",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.3.5",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/mapbox-gl": "^2.7.18",
    "@types/node": "^24.9.1",
    "@types/react": "19.1.8",
    "autoprefixer": "^10.4.14",
    "eslint": "9.30.1",
    "eslint-config-next": "15.3.5",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-sitemap": "4.2.3",
    "postcss": "^8.4.21",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ]
  },
  "prisma": {
    "seed": "node --loader ts-node/esm ./prisma/seed.ts"
  }
}