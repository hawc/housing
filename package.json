    "clsx": "^1.2.1",
    "lucide-react": "^0.257.0",
    "mapbox-gl": "^2.15.0",
    "next": "^13.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-map-gl": "^7.1.2",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.13.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
